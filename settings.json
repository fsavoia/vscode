        "sqlalchemy",
        "sqlmodel",
        "termcolor",
        "Terrafriend",
        "tracebacks",
        "typer",
        "upserv",
        "usefixtures"
    ],
    "explorer.confirmDelete": false,
    "terminal.integrated.copyOnSelection": true,
    "editor.minimap.enabled": false,
    "editor.fontFamily": "Cascadia Code",
    "editor.fontLigatures": true,
    "terminal.integrated.fontFamily": "MesloLGMDZ Nerd Font",
    "git.confirmSync": false,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "symbols.hidesExplorerArrows": false,
    "update.mode": "manual",
    "remote.extensionKind": {
        "GitHub.copilot": [
            "ui"
        ],
        "GitHub.copilot-chat": [
            "ui"
        ],
    },
    "snyk.advanced.cliPath": "/Users/fsavoia/Library/Application Support/snyk-ls/snyk-macos",
    "snyk.yesWelcomeNotification": false,
    "terminal.integrated.scrollback": 2000,
    "snyk.trustedFolders": [
        "/Users/fsavoia/Dev/ArjanCodes",
        "/Users/fsavoia/Dev/Go"
    ],
    "window.density.editorTabHeight": "compact",
    "workbench.colorTheme": "Python Theme",
    "go.toolsManagement.autoUpdate": true,
    "editor.renderLineHighlight": "gutter",
}
